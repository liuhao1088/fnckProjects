/* pages/fengniaoguoji/fengniaoguoji.wxss */
.mian {
  width: 100%;
  background-color: #FEE504;
  padding-bottom: 40rpx;
}

.nav_div {
  position: fixed;
  left: 20rpx;
  width: 180rpx;
  height: 60rpx;
  border-radius: 40rpx;
  background-color: white;
  opacity: 0.6;
  display: flex;
  justify-content: center;
  align-items: center;
}

.nav_maxdiv {
  width: 70%;
  height: 100%;
  border: 0rpx solid black;
}

.nav_iocndiv {
  float: left;
  height: 100%;
  width: calc(100%/2);
  line-height: 74rpx;
  font-size: 44rpx;
  text-align: center;

}

.iocn {
  height: 100%;
  float: left;
}

.side {
  float: right;
  height: 50%;
  margin-top: 16rpx;
  border: 1rpx solid #83919D;
}

.img_div {
  width: 100%;
  height: 400rpx;
  border: 0rpx solid red;
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.img{
  height: 90%;
  width: 90%;
  border: 0rpx solid black;
}

.img image{
  height: 100%;
  width: 100%;
}